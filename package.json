{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:emulator": "concurrently --kill-others \"npm run firebase:emulator\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export -o build",
    "deploy:withoutFunctions": "npm run export && firebase deploy --only hosting,storage,firestore",
    "deploy:withFunctions": "npm run export && cd ./functions && npm run setEnv && cd .. && firebase deploy",
    "firebase:emulator": "firebase emulators:start",
    "firebase:hosting": "npm run export && firebase deploy --only hosting",
    "firebase:hosting:preview": "npm run export && firebase hosting:channel:deploy preview",
    "firestore:rules": "firebase deploy --only firestore:rules",
    "test:firestore:rules": "firebase emulators:exec --only firestore \"cd test && cd firestore && npm run test\"",
    "test:e2e": "playwright test --project chromium",
    "test:e2e:allBrowsers": "playwright test",
    "test:e2e:codegen": "playwright codegen localhost:3000"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "@mui/material": "^5.3.1",
    "@reduxjs/toolkit": "^1.7.1",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "antd": "^4.18.5",
    "axios": "^0.25.0",
    "dotenv": "^14.3.2",
    "firebase": "^9.6.6",
    "hamburger-react": "^2.4.1",
    "immer": "^9.0.6",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^12.0.7",
    "next-fonts": "^1.5.1",
    "openai-api": "^1.2.6",
    "rc-cascader": "^3.2.1",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^5.0.2",
    "react-hook-form": "^7.25.3",
    "react-hot-toast": "^2.1.1",
    "react-icons": "^4.3.1",
    "react-popper": "^2.2.5",
    "react-redux": "^7.2.6",
